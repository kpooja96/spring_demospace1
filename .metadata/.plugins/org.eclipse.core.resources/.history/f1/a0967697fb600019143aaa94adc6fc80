package com.capgemini.manytomany;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.JoinTable;
import javax.persistence.ManyToMany;

@Entity
public class project {

	@Id
	private int projectId;
	private String projectName;
	
	@ManyToMany
	@JoinTable(name="employee_project",joinColumns=@JoinColumn(name = "projectId"),inverseJoinColumns=@JoinColumn(name = "employee_id"))
	private Employee employee;
	
	public project() {
		super();
	}

	
	public project(int projectId, String projectName) {
		super();
		this.projectId = projectId;
		this.projectName = projectName;
	}


	

	public int getProjectId() {
		return projectId;
	}

	public void setProjectId(int projectId) {
		this.projectId = projectId;
	}

	public String getProjectName() {
		return projectName;
	}

	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}

	

	@Override
	public String toString() {
		return "project [projectId=" + projectId + ", projectName=" + projectName + ", employee=" + employee + "]";
	}
	
	
}
